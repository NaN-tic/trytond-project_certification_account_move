<data>
    <xpath expr="/form/notebook/page[@name='invoiced_progress']"
        position="before">
        <page name="revenue_moves">
            <field name="revenue_moves" colspan="4"/>
        </page>
    </xpath>

    <xpath expr="/form/notebook/page/field[@name='invoiced_amount']" position="after">
      <label name="revenue_merited"/>
      <field name="revenue_merited"/>
      <label name="revenue_pending_merited"/>
      <field name="revenue_pending_merited"/>
    </xpath> 
</data>
